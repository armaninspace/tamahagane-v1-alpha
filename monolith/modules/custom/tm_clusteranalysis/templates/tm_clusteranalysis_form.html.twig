<form class="sections_forms" id="text_cluster_form" method="post" style="display: block;">
  <h3>Text Cluster</h3>
  <div class="form-inner">
    <div class="form-group">
	  <label for="tcluster_title" class="control-label">Title:<span class="form-required" title="This field is required."></span></label>
	  <input type="text" class="form-control" id="tcluster_title" name="tcluster_title" required pattern=".*\S+.*">
    </div>
    <div class="form-group full-width">
	  <label for="tcluster_xaxis_col" class="control-label">Select Value(s):<span class="form-required" title="This field is required."></span></label>
	  <select class="selectpicker multiselect" id="tcluster_xaxis_col" name="tcluster_xaxis_col" multiple required>
	  {% for data in all_types.chars_type %}
	    {% set foo = data|split('=') %}
	    <option data-type="{{ foo[1] }}" value="{{ foo[0] }}">{{ foo[0] }}</option>
	  {% endfor %}
	  </select>
	</div>
	  <input type="hidden" name="img_dir" id="imgDir" value="{{ img_dir }}">
	  <input type="hidden" name="csv_file_path" id="csvFilePath" value="{{ file_path }}">
	  <input type="hidden" id="module_path_line" name="module_path_clusteranalysis" value="{{ module_path }}">

	<div style="display: none">
	<div class="form-group filter-div">
	  <label for="" class="control-label filters">Filters:</label>
	  <div class="filter_note"><span class="form-required"></span>Character filters are case sensitive for "is equal to" operator.</div>
	</div>
    <input type="hidden" name="tcluster_filter_count" id="tcluster_filter_count" value="1"/>
	<div id="tcluster_filter1" class="form-group form-inline filterDiv">
	  <select class="form-control filterColoumSel" id="tcluster_filter_sel1" name="tcluster_filter_sel1">
		<option data-type="" value="">--- Select Filter ----</option>
		{% for data in merged_fields %}
		  {% set foo = data|split('=') %}
		  <option data-type="{{ foo[1] }}" value="{{ foo[0] }}">{{ foo[0] }}</option>
		{% endfor %}
	  </select>
	  <select class="form-control" id="tcluster_filter_cond_sel1" name="tcluster_filter_cond_sel1">
        <option value="">--Select Operator--</option>
	  </select>
      <input type="text" class="form-control" id="tcluster_filter_cond_val1" name="tcluster_filter_cond_val1" placeholder="Value">
	</div>
	<div class="form-group  add-more-cont">
      <a id="tcluster_addb" class="add-more" name="tcluster_addmore" type="">Add Condition</a>
	</div>
	  </div>
  </div><!-- end form-inner-->
  <div class="form-group form-btns">	
	<input type="submit" class="btn btn-primary submit-btn" name="tcluster_sub" value="Submit" id="tcluster_sub" >
<button type="button" class="btn btn-default cancel_btn">Cancel</button>
<img class="classifier-modify" src="/sites/default/files/graphloader.gif">
  </div>
 <input id="txtclusterfile" type="hidden" name="txtclusterfile" value="">
</form>